@inproceedings{goleanuDesignOptimizationTechniques2011,
	title = {Design and optimization techniques for the current return path in a composite aircraft},
	url = {https://ieeexplore.ieee.org/abstract/document/6020581},
	abstract = {The emergence of new composite materials in the aeronautical domain requires a paradigm shift in the electrical system design, especially concerning the current return path. The composite fuselage cannot comply with the required functionalities, as the aluminium skin does, due to its heating and conductive natural limitations. The aim of this paper is to propose some solutions for the design and the optimisation of the current return path in this new aeronautical environment, using a complex modelling method (PEEC-AMLFMM) coupled with, firstly, a path finding algorithm followed by an optimization process, secondly, with a global routing tool followed by an optimisation process and, thirdly, with a multi-agent system approach. A comparison between these methods is made. The first results obtained using our methodologies are promising, providing some solutions for the current return path in a composite aircraft.},
	urldate = {2024-09-23},
	booktitle = {Proceedings of the 2011 14th {European} {Conference} on {Power} {Electronics} and {Applications}},
	author = {Goleanu, Anca-Lucia and Guichon, Jean-Michel and Dunand, Michel and Glize, Pierre and Freycon, Arthur and Schanen, Jean-Luc and Coulomb, Jean-Louis},
	month = aug,
	year = {2011},
	keywords = {/unread, Aircraft, aircraft, Algorithm design and analysis, Composite materials, electrical network design, graph theory, multi-agent based system, optimization, Optimization, Routing, routing algorithms, Steiner trees, Topology},
	pages = {1--10},
	file = {IEEE Xplore Abstract Record:E\:\\Zotero\\documents\\storage\\3RTX2M3J\\6020581.html:text/html;IEEE Xplore Full Text PDF:E\:\\Zotero\\documents\\storage\\UR96KFUI\\Goleanu 等 - 2011 - Design and optimization techniques for the current return path in a composite aircraft.pdf:application/pdf},
